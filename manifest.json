---
layout: null
---
{% assign pwa = site.data.pwa %}
{
  "name": "{{ pwa.name }}",
  "short_name": "{{ pwa.shortName }}",
  "description": "{{ pwa.description }}",
  "icons": [{
      "src": "{{ pwa.icon16 }}",
      "sizes": "16x16",
      "type": "image/png"
  },
  {
      "src": "{{ pwa.icon32 }}",
      "sizes": "32x32",
      "type": "image/png"
  },
  {
      "src": "{{ pwa.icon192 }}",
      "sizes": "192x192",
      "type": "image/png"
  },
  {
      "src": "{{ pwa.icon512 }}",
      "sizes": "512x512",
      "type": "image/png"
  }],
  "start_url": "{{ pwa.startUrl }}",
  "display": "{{ pwa.display }}",
  "background_color": "{{ pwa.bgColour }}",
  "theme_color": "{{ pwa.themeColour }}"
}
